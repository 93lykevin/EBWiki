<p id="notice"><%= notice %></p>

<h1>Listing Agencies</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Street address</th>
      <th>City</th>
      <th>State</th>
      <th>Zipcode</th>
      <th>Description</th>
      <th>Telephone</th>
      <th>Email</th>
      <th>Website</th>
      <th>Lead officer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @agencies.each do |agency| %>
      <tr>
        <td><%= agency.name %></td>
        <td><%= agency.street_address %></td>
        <td><%= agency.city %></td>
        <td><%= agency.state_id %></td>
        <td><%= agency.zipcode %></td>
        <td><%= agency.description %></td>
        <td><%= agency.telephone %></td>
        <td><%= agency.email %></td>
        <td><%= agency.website %></td>
        <td><%= agency.lead_officer %></td>
        <td><%= link_to 'Show', agency %></td>
        <td><%= link_to 'Edit', edit_agency_path(agency) %></td>
        <td><%= link_to 'Destroy', agency, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Agency', new_agency_path %>
