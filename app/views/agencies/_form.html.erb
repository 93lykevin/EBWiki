<%= simple_form_for(@agency) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :street_address %>
    <%= f.input :city %>
    <%= f.collection_select :state_id, State.all, :id, :name, {prompt: "State"}, label:"State" %>
    <%= f.input :zipcode %>
    <%= f.input :description %>
    <%= f.input :telephone %>
    <%= f.input :email %>
    <%= f.input :website %>
    <%= f.input :lead_officer %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>